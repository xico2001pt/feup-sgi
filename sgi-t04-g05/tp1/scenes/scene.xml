                <transformationref id="tr_street_ground_walkway_right" />
            </transformation>
            <materials>
                <material id="white" />
            </materials>
            <texture id="tex_concrete" length_s="1.0" length_t="0.1"/>
            <children>
                <componentref id="p_cube_without_base"/>
