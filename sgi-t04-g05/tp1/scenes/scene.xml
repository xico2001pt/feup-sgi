            <texture id="inherit"/>
            <children>
                <componentref id="street_ground_sign_top_outer"/>
                <componentref id="street_ground_sign_top_outer_flipped"/>
                <componentref id="street_ground_sign_top_inner"/>
                <componentref id="street_ground_sign_cylinder"/>
            </children>
        </component>

        <component id="street_ground_sign_top_outer_flipped">
            <transformation>
                <transformationref id="tr_rotateY180" />
            </transformation>
            <materials>
                <material id="white" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id="street_ground_sign_top_outer"/>
            </children>
        </component>

        <component id="street_ground_sign_top_outer">
            <transformation>
            </transformation>
            <materials>
                <material id="red" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id="p_upside_down_triangle"/>
            </children>
